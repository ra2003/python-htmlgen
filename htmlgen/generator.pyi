import collections
import typing
from typing import Union, AnyStr, Iterator, Optional, List, Iterable

class Generator(collections.Iterable):
    def __iter__(self) -> Iterator[bytes]: ...
    def __str__(self) -> str: ...
    def generate(self) -> typing.Generator[Union[AnyStr, "Generator"], None, None]: ...

class NullGenerator(Generator):
    ...

class IteratorGenerator(Generator):
    def __init__(self, iterator: Iterator[Union[AnyStr, Generator]]) -> None: ...

class ChildGenerator(Generator):
    def __init__(self) -> None: ...
    def __len__(self) -> int: ...
    def append(self, child: Optional[Union[AnyStr, Generator]]) -> None: ...
    def extend(self, children: Iterable[Union[AnyStr, Generator]]) -> None: ...
    def remove(self, child: Union[AnyStr, Generator]) -> None: ...
    def empty(self) -> None: ...
    @property
    def children(self) -> List[Union[AnyStr, Generator]]: ...

class HTMLChildGenerator(Generator):
    def __init__(self) -> None: ...
    def __len__(self) -> int: ...
    def append(self, child: Optional[Union[AnyStr, Generator]]) -> None: ...
    def append_raw(self, child: Union[AnyStr, Generator]) -> None: ...
    def extend(self, children: Iterable[Union[AnyStr, Generator]]) -> None: ...
    def extend_raw(self, children: Iterable[Union[AnyStr, Generator]]) -> None: ...
    def remove(self, child: Union[AnyStr, Generator]) -> None: ...
    def remove_raw(self, child: Union[AnyStr, Generator]) -> None: ...
    def empty(self) -> None: ...
    @property
    def children(self) -> List[Union[AnyStr, Generator]]: ...

def generate_html_string(s: Union[AnyStr, Generator]) -> HTMLChildGenerator: ...

class JoinGenerator(ChildGenerator):
    def __init__(self, glue: Union[AnyStr, Generator], pieces: Optional[Iterable[Union[AnyStr, Generator]]] = ...) -> None: ...

class HTMLJoinGenerator(HTMLChildGenerator):
    def __init__(self, glue: Union[AnyStr, Generator], pieces: Optional[Iterable[Union[AnyStr, Generator]]] = ...) -> None: ...
